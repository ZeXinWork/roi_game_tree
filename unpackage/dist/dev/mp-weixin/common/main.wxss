/* 水平间距 */
page {
  background: #f7f7f7;
}
.uni-card {
  border-radis: 10px !important;
}
.uni-card .uni-card__header {
  margin: 0 15px;
  padding: 0 !important;
}
.uni-card .uni-card__header .uni-card__header-title-text {
  padding: 15px 0 12px;
  font-size: 14px;
  flex: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-weight: bold;
}
.uni-card .uni-card__header .uni-card__header-extra-text {
  color: #333;
  font-size: 13px;
  margin: 0 0 0 16px !important;
  flex: none !important;
}
.uni-card .uni-border-bottom::after {
  border-bottom-color: #f5f5f5 !important;
}
.uni-card .uni-card__content {
  padding: 15px 12px 12px !important;
}
.uni-border::after {
  border: none !important;
}
.uni-card-header-wrap {
  display: flex;
  position: relative;
  flex-direction: row;
  align-items: center;
  font-size: 14px;
  padding: 0 0 12px 0;
  border-bottom: 1px solid #f5f5f5;
}
.uni-card-header-wrap .uni-card__header-title-text {
  flex: 1;
  color: #333;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-weight: bold;
}
.uni-card-header-wrap .uni-card__header-extra-view {
  font-size: 13px;
  margin: 0 0 0 8px;
}
.uni-card-header-wrap .uni-card__header-extra-text {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 80px;
  font-size: 13px;
  margin: 0 0 0 8px;
  text-align: right;
}
image {
  width: 100%;
  height: 100%;
  background-size: cover;
}

